<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
        PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <servlet>
        <servlet-name>TableServiceImpl</servlet-name>
        <servlet-class>com.mycomp.server.TableServiceImpl</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TableServiceImpl</servlet-name>
        <url-pattern>/com.mycomp.TableGWT/TableService</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>TableGWT.html</welcome-file>
    </welcome-file-list>
</web-app>